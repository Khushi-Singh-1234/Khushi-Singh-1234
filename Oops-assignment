#creating class Person with methods
class Person:
def __init__(self,name,age,gender):
  self.name=name
  self.age=age
  self.gender=gender

def introduce(self):
  print(f"Hi! My name is {self.name} and I am {self.age} year old .I am {self.gender}.")

per=Person("Khushi Singh",20,"female")
per.introduce()
